
# Local machines

Host bbb
  HostName 10.0.0.12
  User debian
  IdentityFile ~/.ssh/bbb_id_rsa

Host ubuntu
  HostName aonr.de
  User ryohei
  IdentityFile ~/.ssh/id_rsa

Host ubuntu-local
  HostName aonr.de
  User ryohei
  IdentityFile ~/.ssh/id_rsa

Host raspberrypi.local
  HostName raspberrypi.local
  User pi
  IdentityFile ~/.ssh/id_rsa

