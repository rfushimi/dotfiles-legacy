
# Local machines

Host ubuntu
  HostName 10.0.2.100
  User ryohei
  Port 20022
  IdentityFile ~/.ssh/id_ed25519

Host ubuntu-remote
  HostName aonr.de
  User ryohei
  Port 20022
  IdentityFile ~/.ssh/id_ed25519

Host ubuntu-sudo
  HostName 127.0.0.1
  User root
  IdentityFile ~/.ssh/id_ed25519
  ProxyJump ubuntu

Host raspberrypi.local
  HostName raspberrypi.local
  User pi
  IdentityFile ~/.ssh/id_rsa

Host nas
  Hostname 10.0.0.11
  User ryohei
  Port 20022
  IdentityFile ~/.ssh/id_ed25519
  ProxyJump ubuntu
