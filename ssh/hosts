
# Local machines

Host ubuntu
  HostName aonr.de
  User ryohei
  Port 20022
  IdentityFile ~/.ssh/id_ed25519

Host ubuntu-local
  HostName aonr.de
  User ryohei
  IdentityFile ~/.ssh/id_ed25519

Host ubuntu-sudo
  HostName 127.0.0.1
  User root
  IdentityFile ~/.ssh/id_ed25519
  ProxyJump ubuntu

Host raspberrypi.local
  HostName raspberrypi.local
  User pi
  IdentityFile ~/.ssh/id_rsa


